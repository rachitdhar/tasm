//	data_types.tasm
//
//	Currently there are just two data types:
//	(1) UINT
//	(2) CHAR
//
//	Every source number (either an address or value)
//	could be in hex / oct / decimal format
//
//	Every char/string is written inside "".
//
//	The '\' is used as an escape character.
//	\n and \r are defined currently.

put	0x186a0		"hello world\n"
put	0x186ad		15
out
